  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pallad-ts/app-env/issues"
  },
  "homepage": "https://github.com/pallad-ts/app-env#readme",
  "devDependencies": {
    "@pallad/scripts": "^4",
    "@types/node": "^18",
    "conditional-type-checks": "^1",
    "sinon": "^20"
  },
  "dependencies": {
    "@pallad/builder": "^2"
  }
